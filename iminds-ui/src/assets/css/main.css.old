/*for loader*/

#loader {
    z-index: 99999;
    width: 100%;
    display: none;
    left: 0;
    position: fixed;
    background: rgba(255, 255, 255, 0.72);
    height: 100%;
    top: 0;
}

@-webkit-keyframes spin {
    0% {
        -webkit-transform: rotate(0deg);
    }
    100% {
        -webkit-transform: rotate(360deg);
    }
}

@keyframes spin {
    0% {
        transform: rotate(0deg);
    }
    100% {
        transform: rotate(360deg);
    }
}


