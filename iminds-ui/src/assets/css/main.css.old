    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    border-left: 2px solid;
}

.tree li {
    margin: 0;
    padding: 0 1.5em;
    /* indentation + .5em */
    line-height: 2em;
    /* default list item's `line-height` */
    position: relative;
    font-family: 'OpenSans-Semibold';
}

.tree li:before {
    content: "";
    display: block;
    width: 10px;
    /* same with indentation */
    height: 0;
    /* border-top:2px solid; */
    margin-top: -1px;
    /* border top width */
    position: absolute;
    top: 1em;
    /* (line-height/2) */
    left: 0;
}

.que-tree li:before, .flathierarchy li:before {
    border-top: 2px solid;
}

.tree li:last-child:before {
    background: white;
    /* same with body background */
    height: auto;
    top: 1em;
    /* (line-height/2) */
    bottom: 0;
}

.treeview li span:hover+ul li span, .treeview ul li span:hover+ul li span {
    background-color: #eee;
    padding: 3px;
    border-radius: 5px;
}

.label-header, .form-group label{
    font-family: 'OpenSans-Semibold';
    margin: 0;
    color: #005197;
}

